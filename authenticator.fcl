RULEBLOCK rulesFromAssociationRules
	ACT : MIN;
	ACCU : MAX;
	AND : MIN;
	RULE 16513 : IF meanDwellTime IS lessMedium AND (meanFlightTime IS low AND (meanDiGraphKUTime IS medium AND (meanDiGraphKDTime IS medium AND (meanTriGraphKDTime IS high AND (mistakesFrequency IS veryLow AND meanDelBackspDwell IS veryLow))))) THEN user IS GENUINE;
